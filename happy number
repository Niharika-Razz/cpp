#include<bits/stdc++.h>
using namespace std;
 int happy(int num)
 {
 	int r,sum=0;
 	while(num!=0)
 	{
        r=num%10;
        sum=sum+r*r;
        num=num/10;
 	}
 	if(sum<=9)
 	{
 		return sum;
 	}
 	else
 	{
 		return happy(sum);
 	}
 }
 int main()
 {
 	int n;
 	cin>>n;
 	int s=happy(n);
 	if(s==1 || s==7)
 	{
 		cout<<"happy";
 	}
 	else
 	{
 		cout<<"not happy";
 	}
 }
